<?xml version="1.0" encoding="utf-8"?>

<androidx.leanback.widget.TitleView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/browse_title_group"
    style="?attr/browseTitleViewStyle"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" >

    <ImageButton
        android:id="@+id/tv_channel_more_img_btn"

        android:layout_width="50dp"
        android:layout_height="50dp"
        android:scaleType="fitXY"
        android:layout_gravity="center_vertical|end"
        android:text="@string/history_clear_all"
        android:backgroundTint="@color/selector_transparent_btn_focus"
        android:fontFamily="@font/mochiypoppone"
        android:textColor="@color/white"
        android:src="@drawable/ic_more"
        />





</androidx.leanback.widget.TitleView>